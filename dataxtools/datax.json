                "parameter": {
                    "column": ['*'],
                    "connection": [{
                        "jdbcUrl": [
                            "jdbc:mysql://reader-host:reader-port/reader-database"
                        ],
                        "table": [
                            "table-place"
                        ]
                    }],
                    "username": "reader-username",
                    "password": "reader-password",
